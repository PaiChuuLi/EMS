@model List<EVEStandard.Models.CharacterMining>

@{
    ViewData["Title"] = "Home Page";
}

<div>
    Access token @ViewData["access_token"] <br />
    Expire in @ViewData["expires_in"]<br />
    Refresh token @ViewData["refresh_token"]<br />
    Expires @ViewData["expires"]<br />
</div>

<div>
    Character ID @ViewData["CharacterID"]<br />
    Character Name @ViewData["CharacterName"]<br />
    Expires On @ViewData["ExpiresOn"] <br />
    Scopes @ViewData["Scopes"]<br />
    Token type @ViewData["TokenType"]<br />
    Character Owner Hash @ViewData["CharacterOwnerHash"] <br />
    <img src="@ViewData["image"]" />
</div>


<div>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <td><strong>Количество</strong></td>
                <td><strong>ID</strong></td>
            </tr>
        </thead>
        <tbody>
            @foreach (EVEStandard.Models.CharacterMining cm in Model)
            {
                <tr>
                    <td>@cm.Quantity</td>
                    <td>@cm.TypeId</td>
                </tr>
            }
        </tbody>
        @*<tfoot>
            <tr>
                <td colspan="2">
                    <span data-bind="click:previousPage,visible:currentPage() > 1" class="glyphicon glyphicon-circle-arrow-left" style="cursor:pointer;"></span>
                    <span>Страница:</span><span data-bind="text:currentPage"></span><span> из</span> <span data-bind="text:lastPage"></span>
                    <span data-bind="click:nextPage,visible:currentPage() < lastPage" class="glyphicon glyphicon-circle-arrow-right" style="cursor:pointer;"></span>
                </td>
            </tr>
        </tfoot>*@
    </table>   
</div>


    <div>
        @ViewData["emessage"]
    </div><div>
        @ViewData["error"]
    </div><div>
        @ViewData["data"]
    </div>